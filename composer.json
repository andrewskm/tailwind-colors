{
    "name": "andrewskm/utility-colors",
    "description": "List of color names for utility class generation.",
    "type": "library",
    "homepage": "https://github.com/andrewskm/utility-colors",
    "license": "MIT",
    "authors": [
        {
            "name": "Keith Andrews",
            "email": "keith@geerservices.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": ">=9.6.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Andrewskm\\UtilityColors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Andrewskm\\UtilityColors\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
